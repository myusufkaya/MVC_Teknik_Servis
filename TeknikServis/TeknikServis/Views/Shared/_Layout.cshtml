@{ 
    TeknikServis.Models.Kullanici klnc = (TeknikServis.Models.Kullanici)Session["Kullanici"];
}
<!DOCTYPE html>
<!--
This is a starter template page. Use this page to start your new project from
scratch. This page gets rid of all links and provides the needed markup only.
-->
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>Bayotek | Düzce</title>
    <!-- Font Awesome Icons -->
    <link rel="stylesheet" href="/Content/Tema/plugins/font-awesome/css/font-awesome.min.css">
    <!-- Theme style -->
    <link rel="stylesheet" href="/Content/Tema/dist/css/adminlte.min.css">
    @RenderSection("cssDosyalari", false)
    <!-- Google Font: Source Sans Pro -->
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700" rel="stylesheet">
</head>
<body class="hold-transition sidebar-mini">
    <div class="wrapper">
        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand bg-white navbar-light border-bottom">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#"><i class="fa fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="Index" class="nav-link">Müşteriler</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href='@Url.Action("Arıza","Home")' class="nav-link">Arızalar</a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href='@Url.Action("Malzeme","Home")' class="nav-link">Malzemeler</a>
                </li>
            </ul>
            
        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">
            <!-- Brand Logo -->
            <a href="http://www.bayotek.com" class="brand-link">
                <img src="~/Content/Tema/dist/teknikservis.png" alt="Teknik Servis Logo" class="brand-image img-circle elevation-3"
                     style="opacity: 100">
                <span class="brand-text font-weight-light">Bayotek</span>
            </a>
            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="image">
                        <img src="/Content/Tema/dist/fenerbahce.jpg" class="img-circle elevation-2" alt="User Image">
                    </div>
                    <div class="info">
                       
                            <a href="http://www.myusufkaya.com" class="d-block">@klnc.Kullanici_Adi</a>
                        
                       
                    </div>
                </div>
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item">
                            <a href="/Home/Index" class="nav-link">
                                <i class="nav-icon fa fa-users"></i>
                                <p>
                                    Müşteri Bilgi Ekranı
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Home/Ekle" class="nav-link">
                                <i class="nav-icon fa fa-user-plus"></i>
                                <p>
                                    Müşteri Ekle
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Home/Arıza" class="nav-link">
                                <i class="nav-icon fa fa-wrench"></i>
                                <p>
                                    Arıza Bilgi Ekranı
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Home/ArızaEkle" class="nav-link">
                                <i class="nav-icon fa fa fa-wrench"></i>
                                <p>
                                    Arıza Ekle
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Home/Malzeme" class="nav-link">
                                <i class="nav-icon fa fa-edit"></i>
                                <p>
                                    Malzeme Bilgi Ekranı
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Home/MalzemeEkle" class="nav-link">
                                <i class="nav-icon fa fa-edit"></i>
                                <p>
                                    Malzeme Ekle
                                </p>
                            </a>
                        </li>
                      
                        <li class="nav-item">
                            <a href="/Home/Siparis" class="nav-link">
                                <i class="nav-icon fa fa-truck"></i>
                                <p>
                                    Sipariş
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Home/SiparisEkle" class="nav-link">
                                <i class="nav-icon fa fa-truck"></i>
                                <p>
                                    Sipariş Ver
                                </p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a href="/Login/LogOff" class="nav-link">
                                <i class="nav-icon fa fa-close"></i>
                                <p>
                                    Çıkış Yap
                                </p>
                            </a>
                        </li>
                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Content Header (Page header) -->
            @RenderSection("Baslik", false)

            <!-- /.content-header -->
            <!-- Main content -->
            <div class="content">
                <div class="container-fluid">
                    <div class="container-fluid" style="padding-top:10px;">
                        <div class="row">
                            <div class="col-lg-12">
                                @RenderBody()
                            </div>
                        </div>
                    </div>
                    <!-- /.row -->
                </div><!-- /.container-fluid -->
            </div>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
            <div class="p-3">
                <h5>Title</h5>
                <p>Sidebar content</p>
            </div>
        </aside>
        <!-- /.control-sidebar -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <!-- To the right -->
            <div class="float-right d-none d-sm-inline">
               Düzce Teknik Servis
            </div>
            <!-- Default to the left -->
            <strong>Copyright &copy; 2014-2018 <a href="https://www.myusufkaya.com">myusufkaya.com</a>.</strong> All rights reserved.
        </footer>
    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- jQuery -->
    <script src="/Content/Tema/plugins/jquery/jquery.min.js"></script>
    <!-- Bootstrap 4 -->
    <script src="/Content/Tema/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>
    <!-- AdminLTE App -->
    <script src="/Content/Tema/dist/js/adminlte.min.js"></script>
</body>
</html>
